{
  "name": "@dashly/hooks",
  "version": "0.4.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react --external @mantine/hooks",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react --external @mantine/hooks",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "@mantine/hooks": "^5.10.0",
    "@dashly/tsconfig": "workspace:*",
    "tsup": "^6.5.0",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
